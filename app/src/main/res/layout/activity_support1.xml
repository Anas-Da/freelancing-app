<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:background="#FFFFFFFF"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r0h5yg2kjsuak"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FF18104a"
        android:fillViewport="true"
        android:paddingTop="17dp"
        android:paddingBottom="-3dp">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <ImageView
        android:id="@+id/re7c8b5ltt7l"
        android:layout_width="wrap_content"
        android:layout_height="33dp"
        android:layout_marginHorizontal="21dp"
        android:scaleType="fitXY"
        android:src="@drawable/arrow_back" />
        <TextView
            android:id="@+id/rlzqje35ev5"
            android:textColor="#ffffff"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_toEndOf="@+id/re7c8b5ltt7l"
            android:layout_marginTop="-5dp"
            android:layout_weight="1"
            android:text="Support"
            />
    <RelativeLayout
        android:id="@+id/rdyndsji2gv7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="25dp"
        android:background="@drawable/tl90tr90bffffffff"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:paddingVertical="30dp">


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:background="@drawable/bar"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="-30dp">


            <ImageButton
                android:id="@+id/smile_b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="5dp"
                android:layout_marginTop="7dp"
                android:layout_centerVertical="true"
                android:background="@null"
                android:src="@drawable/smile"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/microphone_b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="7dp"
                android:layout_marginEnd="5dp"
                android:background="@null"
                android:src="@drawable/microphone"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

            <ImageButton
                android:id="@+id/attachment_b"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/microphone_b"
                android:layout_centerVertical="true"
                android:layout_marginTop="7dp"
                android:layout_marginEnd="35dp"
                android:background="@null"
                android:src="@drawable/attachment"
                tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="TouchTargetSizeCheck"
                android:layout_toEndOf="@+id/smile_b"
                android:layout_marginStart="15dp"
                android:layout_centerVertical="true"
                android:hint="Message"
                android:background="@android:color/transparent"
                android:textColor="#D9D9D9"
                />


        </RelativeLayout>
    </RelativeLayout>
    </RelativeLayout>
    </ScrollView>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>