<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:fillViewport="true"
		android:id="@+id/r96dvzwh2hkf"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:background="#FF18104a"
		android:paddingTop="17dp"
		android:clipChildren="false"
		android:clipToPadding="false">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/r60l0uw91jxn"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="22dp"
				android:layout_marginHorizontal="18dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/r62v501g6bam"
					android:layout_width="1dp"
					android:layout_height="wrap_content"
					android:background="#FFffffff"
					android:orientation="vertical">
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rx7daq751yz"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical">
				<TextView
					android:id="@+id/r9psq9aq619a"
					android:textColor="#ffffff"
					android:textSize="16sp"
					android:textStyle="bold"
					android:layout_marginBottom="8dp"
					android:layout_marginStart="65dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Zaher Kolko"
				/>
				<RelativeLayout
					android:id="@+id/rwjzuq5a6k7c"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:clipChildren="false"
					android:clipToPadding="false"
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:clipChildren="false"
						android:clipToPadding="false"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/rqucgbiw25a"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:background="@drawable/tl90tr90bccffffff"
							android:paddingTop="224dp"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/rf40fq0xd3x"
								android:layout_width="139dp"
								android:layout_height="110dp"
								android:background="@drawable/cr20bfff9f9f9"
								android:paddingVertical="16dp"
								android:layout_marginBottom="312dp"
								android:layout_marginHorizontal="113dp"
								android:orientation="vertical">

								<TextView
									android:id="@+id/ranwq0z1rddg"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginHorizontal="8dp"
									android:layout_marginBottom="16dp"
									android:layout_weight="1"
									android:gravity="center"
									android:text="Do you want to start \nthe service with \nZaher Kolko?"
									android:textColor="#000000"
									android:textSize="13sp" />

								<LinearLayout
									android:id="@+id/radxzzux6odn"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:gravity="center_vertical"
									android:layout_marginHorizontal="27dp"
									android:orientation="horizontal">
									<TextView
										android:id="@+id/r9q1373zpwg"
										android:textColor="#78f15a"
										android:textSize="12sp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="Yes"
									/>
									<View
										android:layout_width="0dp"
										android:layout_height="0dp"
										android:layout_weight="1"
									 />
									<TextView
										android:id="@+id/r9y7hhfs68ec"
										android:textColor="#ff0707"
										android:textSize="12sp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="NO"
									/>
								</LinearLayout>
							</LinearLayout>
							<RelativeLayout
								android:id="@+id/r3zp8zgw8pqk_rl"
								android:layout_width="102dp"
								android:layout_height="24dp"
								android:gravity="center_vertical"
								android:layout_marginBottom="7dp"
								>
								<com.google.android.material.imageview.ShapeableImageView
									android:id="@+id/r3zp8zgw8pqk"
									android:layout_width="102dp"
									android:layout_height="24dp"
									android:src="@mipmap/ic_launcher"
									android:scaleType="fitXY"
								/>
								<LinearLayout
									android:paddingVertical="8dp"
									android:paddingLeft="22dp"
									android:paddingRight="10dp"
									android:orientation="vertical"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									>
									<TextView
										android:id="@+id/rb31ewlyamlm"
										android:textColor="#000000"
										android:textSize="10sp"
										android:layout_marginEnd="16dp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="Hello"
									/>
									<TextView
										android:id="@+id/raibfmjdcr28"
										android:textColor="#000000"
										android:textSize="8sp"
										android:layout_height="wrap_content"
										android:layout_width="match_parent"
										android:layout_weight="1"
										android:text="05:42pm"
									/>
								</LinearLayout>
							</RelativeLayout>
							<LinearLayout
								android:id="@+id/r9tqlach46c4"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:gravity="center_vertical"
								android:background="@drawable/tl13tr13bffff4c14"
								android:paddingVertical="9dp"
								android:paddingHorizontal="5dp"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/rg4q899x6qi"
									android:layout_width="30dp"
									android:layout_height="30dp"
									android:layout_marginEnd="8dp"
									android:src="@drawable/smile"
									android:scaleType="fitXY"
								/>
								<TextView
									android:id="@+id/reikr54ohctw"
									android:textColor="#ffffff"
									android:textSize="12sp"
									android:layout_height="wrap_content"
									android:layout_width="wrap_content"
									android:text="Hello"
								/>
								<LinearLayout
									android:id="@+id/rm888xud7cwf"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:layout_weight="1"
									android:orientation="vertical">
								</LinearLayout>
								<ImageView
									android:id="@+id/rkrxsmn1r6t"
									android:layout_width="25dp"
									android:layout_height="25dp"
									android:src="@drawable/message"
									android:scaleType="fitXY"
								/>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>

					<ImageView
						android:id="@+id/rro26iu27"
						android:layout_width="25dp"
						android:layout_height="21dp"
						android:layout_alignParentRight="true"
						android:layout_marginTop="-25dp"
						android:layout_marginEnd="15dp"
						android:scaleType="fitXY"
						android:src="@drawable/list" />
				</RelativeLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
