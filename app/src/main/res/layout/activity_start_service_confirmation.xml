<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:fillViewport="true"
		android:id="@+id/rj2zkhjmwt1k"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:background="#FF18104a"
		android:paddingTop="17dp"
		android:clipChildren="false"
		android:clipToPadding="false">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rctku17fikpf"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="22dp"
				android:layout_marginHorizontal="18dp"
				android:orientation="horizontal">

				<LinearLayout
					android:id="@+id/r405upp3ar7i"
					android:layout_width="22dp"
					android:layout_height="wrap_content"
					android:background="@drawable/sffffffffsw1cr2_6666667461395264bffffffff"
					android:paddingHorizontal="2dp"
					android:layout_marginEnd="1dp"
					android:orientation="vertical"/>
				<LinearLayout
					android:id="@+id/rd4xtnx0bvr4"
					android:layout_width="1dp"
					android:layout_height="wrap_content"
					android:background="#FFffffff"
					android:orientation="vertical">
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/rd0hs5b6p5yu"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical">
				<TextView
					android:id="@+id/rc9nuq8201ni"
					android:textColor="#ffffff"
					android:textSize="16sp"
					android:textStyle="bold"
					android:layout_marginBottom="8dp"
					android:layout_marginStart="65dp"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:text="Zaher Kolko"
				/>
				<RelativeLayout
					android:id="@+id/rkp46bddcqnn"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:clipChildren="false"
					android:clipToPadding="false"
					>
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:clipChildren="false"
						android:clipToPadding="false"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/rycaobeg6a58"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:background="@drawable/tl90tr90bccffffff"
							android:paddingTop="224dp"
							android:orientation="vertical">
							<LinearLayout
								android:id="@+id/rtp79dlfbh3o"
								android:layout_width="139dp"
								android:layout_height="110dp"
								android:background="@drawable/cr20bfff9f9f9"
								android:paddingVertical="16dp"
								android:layout_marginBottom="312dp"
								android:layout_marginHorizontal="113dp"
								android:orientation="vertical">

								<TextView
									android:id="@+id/rs8kvkh953ut"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:layout_marginHorizontal="8dp"
									android:layout_marginBottom="15dp"
									android:layout_weight="1"
									android:gravity="center"
									android:text="Do you want to start \nthe service with \nZaher Kolko?"
									android:textColor="#000000"
									android:textSize="13sp" />
								<LinearLayout
									android:id="@+id/rql1xkkkoqwh"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:gravity="center_vertical"
									android:layout_marginHorizontal="27dp"
									android:orientation="horizontal">
									<TextView
										android:id="@+id/ry48xz7btxfa"
										android:textColor="#78f15a"
										android:textSize="12sp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="Yes"
									/>
									<View
										android:layout_width="0dp"
										android:layout_height="0dp"
										android:layout_weight="1"
									 />
									<TextView
										android:id="@+id/r8johrnhbbwu"
										android:textColor="#ff0707"
										android:textSize="12sp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="NO"
									/>
								</LinearLayout>
							</LinearLayout>
							<RelativeLayout
								android:id="@+id/r6ht0fzo99jc_rl"
								android:layout_width="102dp"
								android:layout_height="24dp"
								android:gravity="center_vertical"
								android:layout_marginBottom="7dp"
								>
								<com.google.android.material.imageview.ShapeableImageView
									android:id="@+id/r6ht0fzo99jc"
									android:layout_width="102dp"
									android:layout_height="24dp"
									android:src="@drawable/received_message_left"
									android:scaleType="fitXY"
								/>
								<LinearLayout
									android:paddingVertical="8dp"
									android:paddingLeft="22dp"
									android:paddingRight="10dp"
									android:orientation="vertical"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									>
									<TextView
										android:id="@+id/rxus5n2srje"
										android:textColor="#000000"
										android:textSize="10sp"
										android:layout_marginEnd="16dp"
										android:layout_height="wrap_content"
										android:layout_width="wrap_content"
										android:text="Hello"
									/>
									<TextView
										android:id="@+id/rb2dp6jao4cs"
										android:textColor="#000000"
										android:textSize="8sp"
										android:layout_height="wrap_content"
										android:layout_width="match_parent"
										android:layout_weight="1"
										android:text="05:42pm"
									/>
								</LinearLayout>
							</RelativeLayout>
							<LinearLayout
								android:id="@+id/r2p5lxtopa5i"
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:gravity="center_vertical"
								android:background="@drawable/tl13tr13bffff4c14"
								android:paddingVertical="9dp"
								android:paddingHorizontal="5dp"
								android:orientation="horizontal">
								<ImageView
									android:id="@+id/r5w31tq6bfvg"
									android:layout_width="30dp"
									android:layout_height="30dp"
									android:layout_marginEnd="8dp"
									android:src="@drawable/smile"
									android:scaleType="fitXY"
								/>
								<TextView
									android:id="@+id/rip7q3pemk8"
									android:textColor="#ffffff"
									android:textSize="12sp"
									android:layout_height="wrap_content"
									android:layout_width="wrap_content"
									android:text="Hello"
								/>
								<LinearLayout
									android:id="@+id/rsbq7ljg5fkj"
									android:layout_width="match_parent"
									android:layout_height="match_parent"
									android:layout_weight="1"
									android:orientation="vertical">
								</LinearLayout>
								<ImageView
									android:id="@+id/r6hhj5y2pbjc"
									android:layout_width="25dp"
									android:layout_height="25dp"
									android:src="@drawable/message"
									android:scaleType="fitXY"
								/>
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>

					<ImageView
						android:id="@+id/rfc79788kc3"
						android:layout_width="25dp"
						android:layout_height="21dp"
						android:layout_alignParentRight="true"
						android:layout_marginTop="-26dp"
						android:layout_marginEnd="15dp"
						android:scaleType="fitXY"
						android:src="@drawable/list" />
				</RelativeLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
