<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<RelativeLayout
		android:background="#FFFFFFFF"
		android:orientation="vertical"
		android:layout_width="match_parent"
		android:layout_height="match_parent">
		<ScrollView
			android:id="@+id/r0h5yg2kjsuak"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_weight="1"
			android:background="#FF18104a"
			android:fillViewport="true"
			android:paddingTop="17dp"
			android:paddingBottom="-3dp">
			<RelativeLayout
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical">

				<ImageButton
					android:id="@+id/back_b"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginHorizontal="21dp"
					android:background="@null"
					android:scaleType="fitXY"
					android:src="@drawable/arrow_back"
					tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
				<TextView
					android:id="@+id/notifications"
					android:textColor="#ffffff"
					android:textSize="20sp"
					android:textStyle="bold"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content"
					android:layout_toEndOf="@+id/back_b"
					android:layout_marginTop="-5dp"
					android:text="Notifications"
					/>

				<RelativeLayout
					android:id="@+id/rdyndsji2gv7"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_alignParentTop="true"
					android:layout_alignParentBottom="true"
					android:layout_marginTop="25dp"
					android:background="@drawable/tl90tr90bffffffff"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:paddingVertical="30dp">

					<androidx.recyclerview.widget.RecyclerView
						android:id="@+id/chats_li"
						android:layout_width="match_parent"
						android:layout_height="550dp"
						android:layout_marginTop="20dp"
						android:padding="16dp"
						android:clipToPadding="true"
						tools:listitem="@layout/item_notification"/>


				</RelativeLayout>
				<RelativeLayout
					android:id="@+id/ahjbfhad"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_alignParentBottom="true"
					android:layout_alignParentStart="true"
					android:layout_alignParentEnd="true"
					android:background="@drawable/bar">
					<ImageButton
						android:id="@+id/account_ib"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerVertical="true"
						android:layout_marginStart="50dp"
						android:background="@null"
						android:src="@drawable/account_page_icon"
						tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>

					<ImageButton
						android:id="@+id/home_ib"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerInParent="true"
						android:background="@null"
						android:src="@drawable/home_page_icon"
						tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
					<ImageButton
						android:id="@+id/chat_ib"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_alignParentEnd="true"
						android:layout_marginEnd="55dp"
						android:background="@null"
						android:layout_centerVertical="true"
						android:src="@drawable/chats_page_icon"
						tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck"/>
				</RelativeLayout>

			</RelativeLayout>
		</ScrollView>
	</RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>