<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFFFF"
        android:orientation="vertical">

        <ScrollView
            android:id="@+id/r0h5yg2kjsuak"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FF18104a"
            android:fillViewport="true"
            android:paddingTop="17dp"
            android:paddingBottom="-3dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/rtif6nzhmkxf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="-3dp"
                    android:layout_marginEnd="2dp"
                    android:text="Sign in"
                    android:textColor="#ffffff"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:translationX="-3dp"
                    android:translationY="-3dp" />

                <TextView
                    android:id="@+id/r9vqib29tvru"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="285dp"
                    android:layout_marginTop="-3dp"
                    android:layout_marginEnd="2dp"
                    android:text="freelancer"
                    android:textColor="#ffffff"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    android:translationX="-10dp"
                    android:translationY="-3dp" />

                <RelativeLayout
                    android:id="@+id/rdyndsji2gv7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="25dp"
                    android:background="@drawable/tl90tr90bffffffff"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical"
                    android:paddingVertical="30dp">

                    <TextView
                        android:id="@+id/rmmhq5zz6np"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="20dp"
                        android:text="Welcome"
                        android:textColor="#000000"
                        android:textSize="20sp"
                        android:textStyle="bold" />

                    <TextView
                        android:id="@+id/rgeio7dyf03"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rmmhq5zz6np"
                        android:layout_marginStart="30dp"
                        android:text="Sign in your account to continue"
                        android:textColor="#96969c"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/rhnpbfnkllv9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rgeio7dyf03"
                        android:layout_marginStart="26dp"
                        android:layout_marginTop="10dp"
                        android:text="Email"
                        android:textColor="#2d2d3a"
                        android:textSize="14sp" />

                    <RelativeLayout
                        android:id="@+id/rx35kc18k1dg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rhnpbfnkllv9"
                        android:layout_marginHorizontal="24dp"
                        android:layout_marginTop="5dp"
                        android:background="@drawable/sfff0f0f0sw1cr50"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="17dp"
                        android:paddingVertical="21dp">

                        <ImageView
                            android:id="@+id/rn6rktg051u"
                            android:layout_width="14dp"
                            android:layout_height="10dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerInParent="false"
                            android:scaleType="fitXY"
                            android:src="@drawable/email"
                            android:translationY="15dp" />

                        <EditText
                            android:id="@+id/email_et"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_toEndOf="@+id/rn6rktg051u"
                            android:ems="18"
                            android:inputType="textEmailAddress"
                            android:hint="username@example.com"
                            android:minHeight="48dp"
                            android:textColor="#060000"
                            android:textSize="12sp"
                            android:translationX="10dp" />
                    </RelativeLayout>

                    <TextView
                        android:id="@+id/ry7kql5e3cnq"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rx35kc18k1dg"
                        android:layout_marginTop="5dp"
                        android:layout_marginStart="28dp"
                        android:text="Password"
                        android:textColor="#2d2d3a"
                        android:textSize="14sp" />

                    <RelativeLayout
                        android:id="@+id/rzqzp5n0yuf"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ry7kql5e3cnq"
                        android:layout_marginTop="5dp"
                        android:layout_marginHorizontal="22dp"
                        android:layout_marginBottom="10dp"
                        android:background="@drawable/sfff0f0f0sw1cr50"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingHorizontal="18dp"
                        android:paddingVertical="19dp">

                        <ImageView
                            android:id="@+id/rjh1fch95zx"
                            android:layout_width="12dp"
                            android:layout_height="14dp"
                            android:layout_marginEnd="13dp"
                            android:scaleType="fitXY"
                            android:src="@drawable/lock"
                            android:translationY="15dp" />

                        <EditText
                            android:id="@+id/password_et"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentTop="true"
                            android:layout_toEndOf="@+id/rjh1fch95zx"
                            android:ems="18"
                            android:hint="password"
                            android:inputType="textPassword"
                            android:minHeight="48dp"
                            android:textSize="12sp" />

                        <ImageView
                            android:id="@+id/eye_b"
                            android:layout_width="19dp"
                            android:layout_height="13dp"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentEnd="true"
                            android:clickable="true"
                            android:scaleType="fitXY"
                            android:src="@drawable/eye"
                            android:translationY="15dp" />
                    </RelativeLayout>

                    <CheckBox
                        android:id="@+id/kajcnslankasl"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rzqzp5n0yuf"
                        android:layout_marginStart="20dp"
                        android:layout_marginBottom="20dp"
                        android:clickable="true" />

                    <TextView
                        android:id="@+id/r8kw3ipzykpi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rzqzp5n0yuf"
                        android:layout_toEndOf="@+id/kajcnslankasl"
                        android:layout_marginTop="15dp"
                        android:layout_marginBottom="30dp"
                        android:text="Remember me"
                        android:textColor="#2d2d3a"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/rt1eognxfvdo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@+id/rzqzp5n0yuf"
                        android:layout_marginTop="15dp"
                        android:layout_marginEnd="30dp"
                        android:clickable="true"
                        android:text="Forgot Password?"
                        android:textColor="#2d2d3a"
                        android:textSize="14sp" />

                    <RadioGroup
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/kajcnslankasl"
                        android:orientation="horizontal"
                        android:layout_marginStart="20dp"
                        android:layout_marginTop="20dp">

                        <RadioButton
                            android:id="@+id/Customer_rb"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="30dp"
                            android:textColor="#18104a"
                            android:textSize="16sp"
                            android:text="Customer" />

                        <RadioButton
                            android:id="@+id/ServiceProvider_rb"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Service Provider"
                            android:textColor="#18104a"
                            android:textSize="16sp" />
                    </RadioGroup>

                    <Button
                        android:id="@+id/SignIn_b"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rzqzp5n0yuf"
                        android:layout_marginHorizontal="24dp"
                        android:layout_marginTop="150dp"
                        android:layout_marginBottom="5dp"
                        android:background="@drawable/button_border"
                        android:backgroundTint="@color/orange"
                        android:gravity="center_horizontal"
                        android:text="Sign In"
                        android:textColor="@color/black" />

                    <TextView
                        android:id="@+id/rzstuld6jjj"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/SignIn_b"
                        android:layout_centerInParent="true"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:clickable="true"
                        android:text="Donâ€™t have an account?"
                        android:textColor="#000000"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/SignUp_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rzstuld6jjj"
                        android:layout_centerInParent="true"
                        android:layout_margin="16dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginHorizontal="24dp"
                        android:clickable="true"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="16dp"
                        android:paddingVertical="17dp"
                        android:text="Sign up"
                        android:textColor="@color/black"
                        android:textSize="15dp" />
                </RelativeLayout>
            </RelativeLayout>
        </ScrollView>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
