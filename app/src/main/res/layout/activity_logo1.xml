<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r0h5yg2kjsuak"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FF18104a"
        android:fillViewport="true"
        android:paddingTop="17dp"
        android:paddingBottom="-3dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/r9vqib29tvru"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="285dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="2dp"
                android:layout_marginBottom="16dp"
                android:text="freelancer"
                android:textColor="#ffffff"
                android:textSize="24sp"
                android:textStyle="bold"
                android:translationY="-5dp" />
            <RelativeLayout
                android:id="@+id/rdyndsji2gv7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="25dp"
                android:layout_marginBottom="0dp"
                android:background="@drawable/tl90tr90bffffffff"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical"
                android:paddingVertical="30dp">
                <ImageView
                    android:id="@+id/rbucznf5me1k"
                    android:layout_width="match_parent"
                    android:layout_height="316dp"
                    android:layout_marginTop="171dp"
                    android:contentDescription="logo Description"
                    android:src="@drawable/logo_image" />
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>