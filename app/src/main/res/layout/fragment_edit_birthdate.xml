<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/rmmhq5zz6np"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:text="@string/edit_your_date_of_birth"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/revw2o8xs2a8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/rmmhq5zz6np"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"
        android:text="Date of Birth"
        android:textColor="#2d2d3a"
        android:textSize="14sp"
        />
    <RelativeLayout
        android:id="@+id/rin922yh8xhr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/revw2o8xs2a8"
        android:layout_marginHorizontal="24dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/sfff0f0f0sw1cr50"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="16dp"
        android:paddingVertical="17dp"
        >

        <ImageView
            android:id="@+id/rtsw53rew2d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:scaleType="fitXY"
            android:src="@drawable/date"
            />

        <EditText
            android:id="@+id/date_of_birth_et"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/rtsw53rew2d"
            android:layout_centerVertical="true"
            android:layout_marginStart="15dp"
            android:ems="18"
            android:hint="@string/date_of_birth"
            android:inputType="date"
            android:minHeight="48dp"
            android:textColor="#060000"
            android:textSize="12sp"
            android:background="@android:color/transparent"/>
    </RelativeLayout>
</RelativeLayout>