<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools">
    <RelativeLayout
        android:background="#FFFFFFFF"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <ScrollView
        android:id="@+id/r0h5yg2kjsuak"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FF18104a"
        android:fillViewport="true"
        android:paddingTop="17dp"
        android:paddingBottom="-3dp">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/rtif6nzhmkxf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="-3dp"
            android:text="Sign up"
            android:textColor="#ffffff"
            android:textSize="24sp"
            android:textStyle="bold"
            android:translationX="-3dp"
            android:translationY="-3dp" />
    <TextView
        android:id="@+id/r9vqib29tvru"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="285dp"
        android:layout_marginTop="-3dp"
        android:layout_marginEnd="2dp"
        android:text="freelancer"
        android:textColor="#ffffff"
        android:textSize="24sp"
        android:textStyle="bold"
        android:translationX="-10dp"
        android:translationY="-3dp" />
    <RelativeLayout
        android:id="@+id/rdyndsji2gv7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="25dp"
        android:background="@drawable/tl90tr90bffffffff"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:orientation="vertical"
        android:paddingVertical="30dp">
        <TextView
            android:id="@+id/create_account_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:text="Create an account"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/rgeio7dyf03"
            android:textColor="#96969c"
            android:textSize="14sp"
            android:layout_marginStart="30dp"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/create_account_tv"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Sign up your account to continue "
            />

        <TextView
            android:id="@+id/rhnpbfnkllv"
            android:textColor="#2D2D3A"
            android:textSize="14sp"
            android:layout_marginStart="26dp"
            android:layout_marginTop="25dp"
            android:layout_below="@+id/rgeio7dyf03"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:text="Account photo"
            />

        <RelativeLayout
            android:id="@+id/account_photo"
            android:layout_width="220dp"
            android:layout_height="220dp"
            android:layout_centerInParent="true"
            android:background="@drawable/cr20bfffdf3f1">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/photo_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:src="@drawable/account_img_big" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/r2r61f10oe76"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="10dp"
            >

            <Button
                android:id="@+id/back_b"
                style="@style/orangeButton"
                android:layout_width="188dp"
                android:layout_height="56dp"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="5dp"
                android:background="@drawable/cr76bffff4c14"
                android:text="@string/back"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="16sp" />

            <Button
                android:id="@+id/next_b"
                style="@style/orangeButton"
                android:layout_width="188dp"
                android:layout_height="56dp"
                android:layout_centerVertical="true"
                android:layout_marginStart="220dp"
                android:layout_marginEnd="5dp"
                android:background="@drawable/cr76bffff4c14"
                android:text="Sign up"
                android:textAllCaps="false"
                android:textColor="@color/white"
                android:textSize="16sp" />
        </RelativeLayout>
    </RelativeLayout>
    </RelativeLayout>
    </ScrollView>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>