<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="#FFFFFFFF"
	android:clipChildren="false"
	android:clipToPadding="false"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent">
	<ScrollView 
		android:fillViewport="true"
		android:id="@+id/rvyfcleqyl3r"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:layout_weight="1"
		android:background="#FF18104a"
		android:paddingTop="17dp"
		android:clipChildren="false"
		android:clipToPadding="false">
		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:clipChildren="false"
			android:clipToPadding="false"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/rx47hcbrvaf"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_vertical"
				android:layout_marginBottom="9dp"
				android:layout_marginHorizontal="21dp"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/r9shtli0wz4s"
					android:layout_width="37dp"
					android:layout_height="33dp"
					android:layout_marginEnd="15dp"
					android:src="@drawable/arrow_back"
					android:scaleType="fitXY"
				/>
				<TextView
					android:id="@+id/rlsdrq4o3a4m"
					android:textColor="#ffffff"
					android:textSize="20sp"
					android:textStyle="bold"
					android:layout_height="wrap_content"
					android:layout_width="match_parent"
					android:layout_weight="1"
					android:text="Support"
				/>
			</LinearLayout>

			<LinearLayout
				android:id="@+id/ru8t768q48d"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="@drawable/tl90tr90bffffffff"
				android:clipChildren="false"
				android:clipToPadding="false"
				android:orientation="vertical"
				android:paddingTop="606dp">



				<LinearLayout
					android:id="@+id/rhd8362etmg"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:background="#FFff4c14"
					android:clipChildren="false"
					android:clipToPadding="false"
					android:gravity="center_vertical"
					android:orientation="horizontal"
					android:paddingHorizontal="5dp"
					android:paddingVertical="9dp">

					<ImageView
						android:id="@+id/r58p3bxkvvf2"
						android:layout_width="30dp"
						android:layout_height="30dp"
						android:layout_marginEnd="4dp"
						android:scaleType="fitXY"
						android:src="@drawable/smile" />

					<TextView
						android:id="@+id/ryyihhck6dad"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="Message"
						android:textColor="#d9d9d9"
						android:textSize="12sp" />

					<LinearLayout
						android:id="@+id/rioborr5p3rs"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:layout_weight="1"
						android:orientation="vertical"></LinearLayout>

					<RelativeLayout
						android:id="@+id/rmbrsq1b93n"
						android:layout_width="30dp"
						android:layout_height="wrap_content"
						android:clipChildren="false"
						android:clipToPadding="false">

						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="match_parent"
							android:clipChildren="false"
							android:clipToPadding="false"
							android:orientation="vertical">

							<ImageView
								android:id="@+id/rjm8goux0bie"
								android:layout_width="match_parent"
								android:layout_height="30dp"
								android:scaleType="fitXY"
								android:src="@drawable/microphone" />
						</LinearLayout>

						<ImageView
							android:id="@+id/r2gtv6zqhdx5"
							android:layout_width="30dp"
							android:layout_height="30dp"
							android:layout_alignParentBottom="true"
							android:layout_marginStart="-27dp"
							android:layout_marginBottom="0dp"
							android:scaleType="fitXY"
							android:src="@drawable/attachment" />
					</RelativeLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
